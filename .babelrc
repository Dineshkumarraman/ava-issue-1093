{
  "sourceMaps": true,
  "presets": [
    "react",
    "es2017",
    "es2016",
    ["es2015", {"modules": false}]
    ],
  "plugins": [
    "transform-class-properties",
    ["transform-object-rest-spread", {
      "useBuiltIns": true
      }],
    ["transform-react-jsx", {
      "useBuiltIns": true
      }],
    ["transform-regenerator", {
      "async": false
      }],
    ["babel-plugin-transform-runtime", {
      "helpers": false,
      "polyfill": false,
      "regenerator": true
      }],
    "transform-export-extensions"
  ]
}
